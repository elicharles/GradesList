@model List<GradeList.Application.DTOs.GradeSubjectDto>

<h1>Grades List Page</h1>
<table>
    <tr>
        <th>ID</th>
        <th>Name</th>
    </tr>
    @foreach (var grade in Model)
    {
        <tr>
            <td>
                @grade.Id
            </td>
            <td>
                @grade.Name
            </td>

        </tr>

    }


</table>
